prefix=@prefix@
exec_prefix=@exec_prefix@
libdir=@libdir@
includedir=@includedir@

Name: CLULibs
Description: CLU Libraries CLU, CLUDraw, CLUParse
Requires: 
Version: @VERSION@
Libs: -L${libdir} -L@CLU_OPENGL@/lib -L@CLU_GLUT@/lib -L@X_LIBS@ -lCLUParse -lCLUDraw -lCLU -lm -lGL -lGLU -lglut -lX11 -lXmu -lXi
Cflags: -I${includedir} -I@CLU_OPENGL@/include -I@CLU_GLUT@/include @GLUT_CXXFLAGS@ @FLTK_CXXFLAGS@ -D_GNUCPP3_
